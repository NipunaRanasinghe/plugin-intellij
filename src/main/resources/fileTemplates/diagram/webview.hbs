        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <style  type = "text/css">
            /* use this class for loader that are shown until the module js is loaded */
            .root-loader {
                position: absolute;
                color: rgba(150, 150, 150, 0.5);
                left: calc(50% - 20px);
                top: calc(50% - 20px);
            }
            {{{styles}}}
        </style>
        <style  type = "text/css">
            {{{themeCss}}}
        </style>
        <style  type = "text/css">
            {{{fontCss}}}
        </style>
        <style type = "text/css">
            {{{disableEdit}}}
        </style>
    </head>

    <body style="overflow: auto;" class="{{{bodyCssClass}}}">
        {{{body}}}
        <script type = "text/JavaScript">
            {{{codePoints}}}
        </script>
        <script type = "text/JavaScript">
                {{{composer}}}
        </script>
        <script type = "text/JavaScript">
            {{{loadedScript}}}
        </script>
        <script type = "text/JavaScript">
            loadedScript();
        </script>
    </body>
</html>