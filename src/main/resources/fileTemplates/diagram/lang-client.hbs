function getLangClient() {
    return {
        isInitialized: true,
        getAST: (params) => {
            return new Promise((resolve, reject) => {
                resolve({ ast: {{{ast}}}, parseSuccess: {{{parseSuccess}}} });
            })
        },
        getEndpoints: () => {
            return new Promise((resolve, reject) => {
                resolve({});
            })
        },
        astDidChange: (params) => {
            window.DiagramPanelBridge.handleDiagramEdit(JSON.stringify(params));
        }
    }
}